{
  "name": "@ansregistry/core",
  "version": "0.1.0",
  "description": "Universal naming for AI agents. Resolve agent:// URIs to service endpoints.",
  "keywords": [
    "ai-agents",
    "naming-system", 
    "agent-discovery",
    "dns-like",
    "typescript",
    "resolver",
    "agent-registry"
  ],
  "homepage": "https://ansregistry.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/ANSRegistry/core.git"
  },
  "bugs": {
    "url": "https://github.com/ANSRegistry/core/issues"
  },
  "author": {
    "name": "Philippe Bolduc",
    "email": "philippe@ansregistry.org",
    "url": "https://ansregistry.org"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:clean": "rm -rf dist && npm run build",
    "build:watch": "tsc --watch",
    "test": "tsx test/resolve.test.ts",
    "test:watch": "tsx --watch test/resolve.test.ts", 
    "test:coverage": "c8 tsx test/resolve.test.ts",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "format": "prettier --write src/ test/",
    "format:check": "prettier --check src/ test/",
    "typecheck": "tsc --noEmit",
    "demo": "serve demo --open",
    "demo:dev": "serve demo --cors",
    "prepublishOnly": "npm run build && npm run test && npm run lint",
    "prepack": "npm run build",
    "semantic-release": "semantic-release"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "tsx": "^4.0.0",
    "serve": "^14.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0", 
    "prettier": "^3.0.0",
    "c8": "^8.0.0",
    "semantic-release": "^22.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/ANSRegistry"
  }
}
